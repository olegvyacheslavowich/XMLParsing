<?xml version="1.0" encoding="UTF-8"?>
<bouq:bouquet xmlns:bouq="http://www.ex.com/bouquet"
        xmlns:xs="www.ex.com/flower"
        xmlns:pack="www.ex.com/packaging"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://www.ex.com/bouquet bouquet.xsd">

    <flowers>
        <orchid>
            <xs:color>Red</xs:color>
            <lifetime>13</lifetime>
            <length>70</length>
            <name>Orchid</name>
        </orchid>
        <rose>
            <xs:color>Black</xs:color>
            <lifetime>15</lifetime>
            <length>100</length>
            <name>Rose</name>
        </rose>
        <tulip>
            <xs:color>Yellow</xs:color>
            <lifetime>10</lifetime>
            <length>50</length>
            <name>Tulip</name>
        </tulip>
        <rose>
            <xs:color>White</xs:color>
            <lifetime>20</lifetime>
            <length>85</length>
            <name>White Rose</name>
        </rose>
        <orchid>
            <xs:color>White</xs:color>
            <lifetime>45</lifetime>
            <length>80</length>
            <name>White Orchid</name>
        </orchid>
    </flowers>
    <packaging>
        <pack:type>envelope</pack:type>
        <pack:color>Brown</pack:color>
    </packaging>

</bouq:bouquet>


