<?xml version="1.0" encoding="UTF-8"?>
<bouq:schema xmlns:bouq="http://www.w3.org/2001/XMLSchema"
             targetNamespace="http://www.ex.com/bouquet">

    <bouq:import namespace="www.ex.com/flower"
                 schemaLocation="flower.xsd"/>
    <bouq:import namespace="www.ex.com/packaging"
                 schemaLocation="packaging.xsd"/>

    <bouq:element name="bouquet" type="bouquet"/>

    <bouq:complexType name="bouquet">
        <bouq:sequence>
            <bouq:element name="flowers" type="flowers"/>
            <bouq:element name="packaging" type="packaging"/>
        </bouq:sequence>
    </bouq:complexType>


</bouq:schema>

